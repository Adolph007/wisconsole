#define($content)
<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h5 class="page-header"><label>添加问题</label></h5>
	<div class="row">
	<form action="addQuestions" method="post" class="form-horizontal">
		<input type="hidden" value="1" name="qstType">
		    <div class="form-group">
			    <label for="inputTitle" class="col-sm-2 control-label">问卷标题</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="inputTitle" placeholder="问卷标题">
			    </div>
			</div>
		    <div class="form-group">
			    <label for="inputAnswerOpt" class="col-sm-2 control-label" >答案选项</label>
			    <div class="col-sm-10">
			    	<input type="text" class="form-control" id="inputAnswerOpt" placeholder="用';'分隔 " />
			    </div>
		    </div>
		    <div class="form-group">
			    <label for="inputRValue" class="col-sm-2 control-label" >反向值</label>
			    <div class="col-sm-2">
			    	<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
					    3 <span class="caret"></span>
					</button>
					  <ul class="dropdown-menu" role="menu">
					    <li><a href="#">3</a></li>
					    <li><a href="#">4</a></li>
					    <li><a href="#">5</a></li>
					    <li><a href="#">6</a></li>
					  </ul>
			    </div>
			    <label for="inputU" class="col-sm-2 control-label" >U值</label>
			    <div class="col-sm-2"><input type="text" class="form-control" id="inputU" placeholder="均值"></div>
			    <label for="inputV" class="col-sm-2 control-label" >V值</label>
			    <div class="col-sm-2"><input type="text" class="form-control" id="inputV" placeholder="方差"></div>
			    
		    </div>
		    <div class="form-group">
			    <label for="inputDirection" class="col-sm-2 control-label" >维度设置</label>
			    <div class="col-sm-10">
			    <div class="row">
			    <div class="col-sm-12">
			    <table class="table">
				  <tr>
				  	<td width="20%">  xxxx </td>
				  	<td>yyy</td>
				  	<td>
				  		<button type="button" class="close">
				  		<span aria-hidden="true">&times;</span>
				  		<span class="sr-only">Close</span>
				  		</button>
				  	</td>
				  </tr>
				  <tr><td>ss1</td><td>ss2</td>
				  <td>
			  		<button type="button" class="close">
			  		<span aria-hidden="true">&times;</span>
			  		<span class="sr-only">Close</span>
			  		</button>
			  	  </td>
				  </tr>
				</table>
				</div>
				</div>
				<div class="row">
					<div class="col-sm-2">
				    		<input type="text" class="form-control" id="directionName" placeholder="名称 " />
				    </div>
				    <div class="col-sm-7">
				    	<input type="text" class="form-control" id="directionExpr" placeholder="计算表达式" />
				    </div>
				    <div class="col-sm-1">
				    <input class="btn btn-default" type="button" value="添加">
				    </div>
				</div>
				</div>
			   
		    </div>
		    
		    <div class="form-group">
			    <div class="col-sm-offset-2 col-sm-10">
			      <button type="submit" class="btn btn-default">下一步</button>
			    </div>
			</div>
		   
	</form>
	</div>
</div>
